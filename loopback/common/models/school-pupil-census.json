{
  "name": "SchoolPupilCensus",
  "description": "Information from the School Pupil Census for Edinburgh.",
  "plural": "SchoolPupilCensus",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mongodb": {
    "collection": "Stats"
  },
  "properties": {
    "id": {
      "type": "number",
      "required": true,
      "id": true
    },
    "year": {
      "type": "number",
      "required": true
    },
    "zone": {
      "type": "string",
      "required": true,
      "index": true
    },
    "age": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "string",
      "required": true
    },
    "group": {
      "type": "string",
      "required": true
    },
    "school": {
      "type": "string",
      "required": true
    },
    "measure": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "dataZone": {
      "type": "belongsTo",
      "model": "GeoJSONFeature",
      "foreignKey": "zone",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
